{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "groceryserver",
      "hostname": "server",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 5000
        },
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    }
  ]
}